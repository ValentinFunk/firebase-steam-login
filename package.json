{
  "name": "firebase-steam-login",
  "version": "1.0.0",
  "main": "dist/server.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "docker-build": "npm run build && docker build -t firebase-steam-login .",
    "serve": "node dist/src/server.js",
    "watch": "npm run build-ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon dist/src/server.js\"",
    "build-ts": "yarn tsc",
    "watch-ts": "tsc -w",
    "postinstall": "npm run build",
    "tslint": "tslint -c tslint.json -p tsconfig.json -e \"node_modules/**/*.*\"",
    "copy-static-assets": "node copy-static-assets.js"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/body-parser": "^1.16.4",
    "@types/compression": "^0.0.33",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.0",
    "@types/errorhandler": "^0.0.30",
    "@types/express": "^4.0.36",
    "@types/express-session": "^1.15.0",
    "@types/express-validator": "^3.0.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.6",
    "@types/passport": "^0.3.3",
    "@types/request-promise": "^4.1.36",
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0",
    "shelljs": "^0.7.8",
    "tslint": "^5.6.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "firebase": "^4.1.3",
    "firebase-admin": "^5.0.0",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-discord": "^0.1.2",
    "passport-steam": "^1.0.8",
    "request-promise": "^4.2.1",
    "rx": "https://registry.yarnpkg.com/rx/-/rx-2.3.24.tgz#14f950a4217d7e35daa71bbcbe58eff68ea4b2b7",
    "typed-promisify": "^0.4.0"
  }
}
